<Window x:Class="Divvun.Installer.OneClick.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Divvun.Installer.OneClick"
        mc:Ignorable="d"
        Loaded="MainWindow_OnLoaded"
        Closing="MainWindow_OnClosing"
        Title="{x:Static local:Strings.AppTitle}" Height="350" Width="600">
    <Grid>
        <Frame Grid.Row="0" x:Name="FrmContainer" NavigationUIVisibility="Hidden" JournalOwnership="OwnsJournal" />
    </Grid>
</Window>